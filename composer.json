{
    "name": "gluehq/craft-texttools",
    "description": "Fixes orphans and removes redundant tags.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "string urldecode",
        "urldecode",
        "php"
    ],
    "support": {
        "docs": "https://github.com/akagibi/stringurldecode/blob/master/README.md",
        "issues": "https://github.com/akagibi/stringurldecode/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Akagibi",
            "homepage": "https://www.akagibi.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
          "gluehq\\texttools\\": "src/"
        }
    },
    "extra": {
        "name": "Text Tools",
        "handle": "texttools",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/akagibi/stringurldecode/master/CHANGELOG.md",
        "class": "gluehq\\texttools\\TextTools"
    }
}
