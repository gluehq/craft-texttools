{
    "name": "gluehq/craft-texttools",
    "description": "Reunites orphans and removes redundant tags.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "filter",
        "text editor",
        "wysiwyg",
        "redactor",
        "redundant tags",
        "oprhans"
    ],
    "support": {
        "docs": "https://github.com/gluehq/craft-texttools/blob/master/README.md",
        "issues": "https://github.com/gluehq/craft-texttools/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Archer",
            "homepage": "https://www.glue.co.nz"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
          "gluehq\\texttools\\": "src/"
        }
    },
    "extra": {
        "name": "Text Tools",
        "handle": "texttools",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/gluehq/craft-texttools/master/CHANGELOG.md",
        "class": "gluehq\\texttools\\TextTools"
    }
}
